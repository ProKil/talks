{
  "name": "slidev-theme-minerva",
  "version": "0.0.2",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/cli": "<0.50.0",
    "@slidev/client": "<0.50.0",
    "@unocss/preset-uno": "^0.64.0",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@slidev/cli": "<0.50.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "fonts": {
        "sans": "Geist",
        "serif": "EB Garamond",
        "mono": "Fira Code"
      }
    }
  }
}